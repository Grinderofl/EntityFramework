{
  "version": "7.0.0-*",
  "description": "Reusable EntityFramework.Core functional tests for provider writers.",
  "compilationOptions": {
    "keyFile": "../../tools/Key.snk"
  },
  "dependencies": {
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-*",
    "EntityFramework.InMemory": "7.0.0-*",
    "xunit": "2.1.0"
  },
  "testRunner": "xunit",
  "commands": {
    "test": "xunit.runner.aspnet"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Globalization": {
          "version": "4.0.0.0",
          "type": "build"
        },
        "System.Text.RegularExpressions": {
          "version": "4.0.0.0",
          "type": "build"
        },
        "System.Threading.Tasks": {
          "version": "4.0.0.0",
          "type": "build"
        }
      }
    },
    "dnx451": {
      "frameworkAssemblies": {
        "System.Collections": "4.0.0.0",
        "System.Diagnostics.Debug": "4.0.0.0",
        "System.Globalization": "4.0.0.0",
        "System.Linq": "4.0.0.0",
        "System.Linq.Expressions": "4.0.0.0",
        "System.Linq.Queryable": "4.0.0.0",
        "System.ObjectModel": "4.0.0.0",
        "System.Reflection": "4.0.0.0",
        "System.Reflection.Extensions": "4.0.0.0",
        "System.Runtime": "4.0.10.0",
        "System.Runtime.Extensions": "4.0.0.0",
        "System.Text.RegularExpressions": "4.0.0.0",
        "System.Threading": "4.0.0.0",
        "System.Threading.Tasks": "4.0.0.0"
      },
      "dependencies": {
        "xunit.runner.aspnet": "2.0.0-aspnet-*"
      }
    },
    "dnxcore50": {
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-*",
        "xunit.runner.aspnet": "2.0.0-aspnet-*"
      }
    }
  }
}
